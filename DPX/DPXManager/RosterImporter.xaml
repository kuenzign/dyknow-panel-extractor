<Window x:Class="DPXManager.RosterImporter"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Import Class Roster" Height="300" Width="618">
    <Grid>
        <ScrollViewer Margin="0,50,0,0">
            <Grid Name="myGrid" Height="Auto" Width="Auto" />
        </ScrollViewer>
        <CheckBox Height="16" Margin="13,11,0,0" Name="checkBoxHeaders" VerticalAlignment="Top" HorizontalAlignment="Left" Width="191" Checked="CheckBoxHeaders_Checked" Unchecked="CheckBoxHeaders_Unchecked">First Line is Headers</CheckBox>
        <Label Height="28" Margin="182,6,256,0" Name="labelNumberOfStudents" VerticalAlignment="Top"></Label>
        <Button Height="23" Margin="0,7.48,17,0" Name="buttonImport" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="ButtonImport_Click">Import</Button>
    </Grid>
</Window>